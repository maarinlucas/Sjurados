{
  "expo": {
    "name": "s_jurados",
    "slug": "s_jurados",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#371b72"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#371b72"
      },
      "package": "com.seuusername.s_jurados",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos para selecionar imagens das batalhas."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5c550b35-c94a-40b1-af67-253768ccde2e"
      }
    }
  }
}
